@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Jewelries")

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
        {
            controller = "Jewelries",
            action = "List",
            category = link,
            page = 1
        },
        new { @class = link == ViewBag.SelectedCategory ? "selected" : null }
    )
}